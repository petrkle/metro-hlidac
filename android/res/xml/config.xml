<?xml version='1.0' encoding='utf-8'?>
<widget id="cz.kle.metro" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Metro hlídač</name>
    <description>
			Obrázky od čtenářů deníku metro.
    </description>
		<author email="petr@kle.cz" href="https://petr.kle.cz">
				Petr Kletečka
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="mailto:*" />
</widget>
